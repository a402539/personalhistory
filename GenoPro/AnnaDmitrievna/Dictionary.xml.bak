	<!-- The _FmtHusbandAndWife tag is used to format a string such "André & Hélène" or "André & Wife" -->
	<_FmtHusbandAndWife T="{0} и {1}" />		<!-- 0=husband name, 1=wife name -->
	<_Husband T="муж" />
	<_Wife T="жена" />
